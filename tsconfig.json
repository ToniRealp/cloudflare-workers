{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "src"
  ]
}
